{
    "manifest_version": 3,
    "name": "Bookmarks Manager",
    "version": "0.0.1",
    "description": "Bookmarks Manager",
    "content_security_policy": {
        "extension_page": "script-src 'self' 'unsafe-eval'; object-src 'self';"
    },
    "icons": {
        "16": "static/img/icon.png",
        "19": "static/img/icon.png",
        "38": "static/img/icon.png",
        "48": "static/img/icon.png",
        "128": "static/img/icon.png"
    },
    "action": {
        "default_icon": "static/img/icon.png",
        "default_title": "Bookmarks Manager",
        "default_popup": "popup.html"
    },
    "options_page": "option.html",
    "background": {
        "service_worker": "background.main.js",
        "type": "module"
    },
    "host_permissions": [
        "*://*/*"
    ],
    "permissions": [
        "storage",
        "bookmarks"
    ]
}